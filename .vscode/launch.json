{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    //
    // See: https://hbfsrobotics.com/blog/configuring-vs-code-arm-development-stm32cubemx
    // See: https://marcelball.ca/projects/cortex-debug/cortex-debug-launch-configurations/
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "openocd",
            "cwd": "${workspaceRoot}",
            "executable": "./gcc/bwhoop.elf",
            "name": "Debug (OpenOCD)",
            "device": "STM32F031x",
            "configFiles": [
                "${workspaceRoot}/openocd/scripts/interface/stlink.cfg",
                "${workspaceRoot}/openocd/scripts/target/stm32f0x.cfg",
            ],
            "svdFile": "${workspaceRoot}/.vscode/STM32F031x.svd"
        },
        {
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "stutil",
            "cwd": "${workspaceRoot}",
            "executable": "./gcc/bwhoop.elf",
            "name": "Debug (ST-Util)",
            "device": "STM32F031x",
            "v1": false,
            "svdFile": "${workspaceRoot}/.vscode/STM32F031x.svd"
        }
    ]
}